{% extends "WinkMurderGameBundle:Layout:base.html.twig" %}

{% block pageTitle %}You{% endblock %}

{% block body %}
    {% if is_granted('ROLE_ADMIN') %}
        You are the administrator.
    {% else %}
        <div>You are {{ player.name }}</div>
        <div><img src="{{ player.imageUrl }}" alt="{{ player.name }}" /></div>
    {% endif %}
{% endblock %}