{
    "name": "plb/wink-murder",

    "repositories": [
        {"type": "composer", "url": "http://composer.webfactory.de"},
        {
            "type": "package",
            "package": {
                "name": "phpflickr/phpflickr",
                "version": "3.1.0",
                "dist": {
                    "url": "http://phpflickr.googlecode.com/files/phpFlickr-3.1.zip",
                    "type": "zip"
                },
                "autoload": {
                    "classmap": [""]
                }
            }
        }
    ],

    "require": {
        "symfony/symfony": "2.1.*",
        "doctrine/orm": "2.3.*",
        "symfony/assetic-bundle": "2.1.x-dev",
        "kriswallsmith/assetic": "1.1.x-dev",
        "webfactory/assetdependenciesbugfix-bundle": "2.1.*",
        "webfactory/compass-bundle": "1.0.*",
        "sensio/framework-extra-bundle": "2.1.x-dev",
        "webfactory/exceptions-bundle": "1.0.*",
        "jlong/sass-twitter-bootstrap": "2.1.0",
        "stof/doctrine-extensions-bundle": "1.1.x-dev",
        "phpflickr/phpflickr": "3.1.*",
        "doctrine/doctrine-migrations-bundle": "dev-master",
        "doctrine/migrations": "dev-master",
        "symfony/monolog-bundle": "2.1.x-dev"
    },

    "require-dev": {
        "phpunit/phpunit": "3.7.8"
    },

    "autoload": {
        "psr-0": {
            "WinkMurder": "src/"
        }
    }

}
